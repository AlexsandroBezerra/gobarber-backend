    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.6",
    "ioredis": "^4.19.2",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.4.6",
    "mongodb": "^3.6.3",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.16",
    "pg": "^8.4.2",
    "rate-limiter-flexible": "^2.1.13",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.4.0",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.1"
